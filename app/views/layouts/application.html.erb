<!DOCTYPE html>
<html>
<head>
  <title>Roommates</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <header>
    <span><%= link_to("Log Out #{User.find_by(id:session[:user_id]).first_name}", "/sessions", method:"delete") unless session[:user_id].nil? %></span>
    <span><%= link_to("Edit My Info", edit_user_path(current_user)) unless session[:user_id].nil? %></span>
  </header>

<div id="wrapper">
  <%= yield %>
</div>

</body>
</html>

